import tools.mix.browser.slider.*

class SlideBar
{
	private var slideMc:MovieClip;
	
	public function SlideBar( $mc:MovieClip )
	{
		slideMc = $mc.attachMovie( "slideMc", "slideMc", 1 );
	}
	
	public function setSlideCallBack ( $cbo:Object, $cbf:String ):Void
	{
		slideMc.setSlideCallBack( $cbo, $cbf );
	}
}