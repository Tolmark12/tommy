import lib.product.AttributeVO;

class lib.product.attribt_interface.drop.DM_UpdateVO extends AttributeVO
{
	public var optionsAr			:Array;
	public var indexOfSelectedItem	:Number;
	public var isOpen				:Boolean;
	public var titleText			:String;
	
	public function DM_UpdateVO ( $ar:Array, $index:Number, $isOpen:Boolean, $tt:String )
	{
		optionsAr		     = $ar;
		indexOfSelectedItem	 = $index;
		isOpen 				 = $isOpen;
		titleText			 = $tt;
	}
}